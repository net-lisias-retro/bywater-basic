  100 REM PURPOSE: VERIFY 'STRIP$' FUNCTION
  110 REM  AUTHOR: HOWARD WULF, AF5NE
  120 REM    DATE: 2017-03-30
  130 REM    NOTE: OPTION VERSION "HAARDT"
  140 REM
  200 PRINT " I", " A", " B"
  210 FOR I = 0 TO 255
  220   LET A$ = CHR$( I )
  230   LET B$ = STRIP$( A$ )
  240   LET A = ASC( A$ )
  250   LET B = ASC( B$ )
  260   PRINT I, A, B
  270 NEXT I
  999 END

