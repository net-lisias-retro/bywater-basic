 10 REM * MAT TRN * TEST PROGRAM
 20 DIM A(3,5), B(5,3)
 30 FOR I=1 TO 3
 40   FOR J=1 TO 5
 50     A(I,J)=10*I+J
 60   NEXT J
 70 NEXT I
 80 MAT B=TRN(A)
 90 FOR J=1 TO 5
100   FOR I=1 TO 3
110     PRINT B(J,I);
120   NEXT I
130   PRINT
140 NEXT J
150 PRINT "'MAT TRN' PASSED THE TEST IF THE NUMBERS 11-15,"
160 PRINT "21-25, AND 31-35 ARE PRINTED IN 3 COLUMNS."
999 END
